package messagerie.back;

import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestParam;
import org.springframework.web.bind.annotation.RestController;

@RestController
@RequestMapping("/user")
public class AuthController {
	
	@PostMapping()
	public void login(@RequestParam String email, @RequestParam String phone, @RequestParam String firstname, @RequestParam String name) {
		
	}
	
	@PostMapping()
	public void register(@RequestParam String email, @RequestParam String phone, @RequestParam String firstname, @RequestParam String name) {
		
	}
}
